{
    "name": "drupalwxt/wxt",
    "description": "Project template for Drupal 8 sites built with the WxT distribution.",
    "type": "drupal-profile",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type"    : "package",
            "package" : {
                "name"    : "wet-boew/wet-boew",
                "version" : "v4.0.23",
                "type"    : "drupal-library",
                "dist"    : {
                    "url"  : "https://github.com/wet-boew/wet-boew-cdn/archive/v4.0.23.tar.gz",
                    "type" : "tar"
                },
                "source"  : {
                    "url"       : "https://github.com/wet-boew/wet-boew-cdn.git",
                    "type"      : "git",
                    "reference" : "v4.0.23"
                }
            }
        },
        {
            "type"    : "package",
            "package" : {
                "name"    : "wet-boew/theme-wet-boew",
                "version" : "v4.0.23",
                "type"    : "drupal-library",
                "dist"    : {
                    "url"  : "https://github.com/wet-boew/themes-cdn/archive/v4.0.23-theme-wet-boew.tar.gz",
                    "type" : "tar"
                },
                "source"  : {
                    "url"       : "https://github.com/wet-boew/themes-cdn.git",
                    "type"      : "git",
                    "reference" : "v4.0.23-theme-wet-boew"
                }
            }
        },
        {
            "type"    : "package",
            "package" : {
                "name"    : "wet-boew/theme-base",
                "version" : "v4.0.23",
                "type"    : "drupal-library",
                "dist"    : {
                    "url"  : "https://github.com/wet-boew/themes-cdn/archive/v4.0.23-theme-base.tar.gz",
                    "type" : "tar"
                },
                "source"  : {
                    "url"       : "https://github.com/wet-boew/themes-cdn.git",
                    "type"      : "git",
                    "reference" : "v4.0.23-theme-base"
                }
            }
        },
        {
            "type"    : "package",
            "package" : {
                "name"    : "wet-boew/theme-gc-intranet",
                "version" : "v4.0.23",
                "type"    : "drupal-library",
                "dist"    : {
                    "url"  : "https://github.com/wet-boew/themes-cdn/archive/v4.0.23-theme-gc-intranet.tar.gz",
                    "type" : "tar"
                },
                "source"  : {
                    "url"       : "https://github.com/wet-boew/themes-cdn.git",
                    "type"      : "git",
                    "reference" : "v4.0.23-theme-gc-intranet"
                }
            }
        },
        {
            "type"    : "package",
            "package" : {
                "name"    : "wet-boew/theme-gcwu-fegc",
                "version" : "v4.0.23",
                "type"    : "drupal-library",
                "dist"    : {
                    "url"  : "https://github.com/wet-boew/themes-cdn/archive/v4.0.23-theme-gcwu-fegc.tar.gz",
                    "type" : "tar"
                },
                "source"  : {
                    "url"       : "https://github.com/wet-boew/themes-cdn.git",
                    "type"      : "git",
                    "reference" : "v4.0.23-theme-gcwu-fegc"
                }
            }
        },
        {
            "type"    : "package",
            "package" : {
                "name"    : "wet-boew/theme-gcweb",
                "version" : "v4.0.23",
                "type"    : "drupal-library",
                "dist"    : {
                    "url"  : "https://github.com/wet-boew/themes-cdn/archive/v4.0.23-gcweb.tar.gz",
                    "type" : "tar"
                },
                "source"  : {
                    "url"       : "https://github.com/wet-boew/themes-cdn.git",
                    "type"      : "git",
                    "reference" : "v4.0.23-gcweb"
                }
            }
        },
        {
            "type"    : "package",
            "package" : {
                "name"    : "wet-boew/theme-ogpl",
                "version" : "v4.0.23",
                "type"    : "drupal-library",
                "dist"    : {
                    "url"  : "https://github.com/wet-boew/themes-cdn/archive/v4.0.23-theme-ogpl.tar.gz",
                    "type" : "tar"
                },
                "source"  : {
                    "url"       : "https://github.com/wet-boew/themes-cdn.git",
                    "type"      : "git",
                    "reference" : "v4.0.23-theme-ogpl"
                }
            }
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "@composer drupal-scaffold"
        ],
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    },
    "config": {
        "secure-http": false,
        "prestissimo": {
            "maxConnections": 10,
            "minConnections": 5
        }
    },
    "extra": {
        "installer-paths": {
            "html/core": [
                "type:drupal-core"
            ],
            "html/libraries/{$name}" : [
                "type:drupal-library"
            ],
            "html/modules/custom/{$name}": [
                "drupal/wxt_library"
            ],
            "html/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "html/profiles/{$name}": [
                "type:drupal-profile"
            ],
            "html/themes/custom/{$name}": [
                "drupal/wxt_bootstrap"
            ],
            "html/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "enable-patching": true,
        "patches": {
            "drupal/blog": {
                "Enter drupal/blog patch #2834732 description here":
                "https://www.drupal.org/files/issues/blog-no_results_text-2834732-2.patch"
            },
            "drupal/core": {
                "Enter drupal/core patch #736066 description here":
                "https://www.drupal.org/files/issues/ajax_js_insert_command-736066-116.patch",
                "1356276 - Allow profiles to provide a base/parent profile and load them in the correct order":
                "https://www.drupal.org/files/issues/1356276-249.patch",
                "Enter drupal/core patch #1838234 description here":
                "https://www.drupal.org/files/issues/1838234-22.patch",
                "Enter drupal/core patch #1918994 description here":
                "https://www.drupal.org/files/issues/1918994-147.patch",
                "2369119 - Fix error on exposed grouped filters on Views save":
                "https://www.drupal.org/files/issues/2369119-145_0.patch",
                "Enter drupal/core patch #2454289 description here":
                "https://www.drupal.org/files/issues/attribute_hreflang_not-2454289-26.patch",
                "Enter drupal/core patch #2630732 description here":
                "https://www.drupal.org/files/issues/implement_entity_fields_migrate-2630732-21.patch",
                "2655104 - List unmet dependencies instead of just failing (Committed to 8.3.x)":
                "https://www.drupal.org/files/issues/2655104-56.patch",
                "2679775 - Fixes float issue with inline label fields (entity references on most cases).":
                "https://www.drupal.org/files/issues/2679775-11-inline-labels.patch",
                "Enter drupal/core patch #2698057 description here":
                "https://www.drupal.org/files/issues/2698057_2.patch",
                "2699157 - Plugin Lazy loading can cause usort warning":
                "https://www.drupal.org/files/issues/Iterating-through-array--2699157.patch",
                "Enter drupal/core patch #2752443 description here":
                "https://www.drupal.org/files/issues/in_correct_order_and-2752443-1.patch",
                "2759397 - Patch EntityReferenceItemNormalizer to prevent recursion":
                "https://www.drupal.org/files/issues/2759397-1-entity_reference_recursion.patch",
                "Enter drupal/core patch #2786577 description here":
                "https://www.drupal.org/files/issues/improve_the_views-2786577-76.patch",
                "Enter drupal/core patch #2816921 description here":
                "https://www.drupal.org/files/issues/user_password-2816921-2.patch"
            },
            "drupal/ctools": {
                "Enter drupal/ctools patch #2712661 description here":
                "https://www.drupal.org/files/issues/allow-2712661-2.patch"
            },
            "drupal/inline_entity_form": {
                "Enter drupal/inline_entity_form patch #2367235 description here":
                "https://www.drupal.org/files/issues/support_entity_revision-2367235-92.patch",
                "Enter drupal/inline_entity_form patch #2673548 description here":
                "https://www.drupal.org/files/issues/inline_entity_form-widget_ux_0.patch"
            },
            "drupal/page_manager": {
                "Enter drupal/page_manager patch #2633052 description here":
                "https://www.drupal.org/files/issues/issue-2633052-2.patch",
                "Enter drupal/page_manager patch #2665328 description here":
                "https://www.drupal.org/files/issues/variants_take_entity-2665328-35.patch",
                "Enter drupal/page_manager patch #2782661 description here":
                "https://www.drupal.org/files/issues/page_variant_for_node-2782661-5.patch"
            },
            "drupal/panels": {
                "Enter drupal/panels patch #2667755 description here":
                "https://www.drupal.org/files/issues/bandaid.patch",
                "Enter drupal/panels patch #2296437 description here":
                "https://www.drupal.org/files/issues/implement_style_plugin-2296437-32.patch"
            },
            "drupal/media_entity": {
                "Enter drupal/media_entity patch #2775131 description here":
                "https://www.drupal.org/files/issues/add_contextual_links-2775131-2.patch"
            },
            "drupal/scheduled_updates": {
                "Enter drupal/scheduled_updates patch #2754467 description here":
                "https://www.drupal.org/files/issues/webprofiler_1.patch"
            },
            "drupal/workbench_moderation": {
                "Enter drupal/workbench_moderation patch #2668006 description here":
                "https://www.drupal.org/files/issues/2668006-2.patch",
                "Enter drupal/workbench_moderation patch #2768917 description here":
                "https://www.drupal.org/files/issues/workbench_moderation_2768917_20.patch"
            }
        }
    },
    "require": {
        "composer/installers": "^1.0",
        "cweagans/composer-patches": "^1.5.0",
        "drupal-composer/drupal-scaffold": "^2.0.0",
        "acquia/lightning": "2.0.3",
        "drupal/admin_toolbar": "1.18",
        "drupal/block_class": "1.0-alpha1",
        "drupal/blog": "2.x-dev",
        "drupal/bootstrap": "3.1",
        "drupal/config_rewrite": "1.0-beta4",
        "drupal/config_update": "1.3",
        "drupal/diff": "1.0-rc1",
        "drupal/entity_reference_revisions": "1.2",
        "drupal/field_group": "1.0-rc6",
        "drupal/linkit": "5.0-beta4",
        "drupal/menu_block": "1.4",
        "drupal/password_policy": "3.x-dev",
        "drupal/simple_sitemap": "2.8",
        "drupal/bootstrap_layouts": "4.1",
        "drupal/paragraphs": "1.0",
        "drupal/field_formatter": "1.0",
        "drupal/file_entity": "2.0-beta3",
        "drupal/media_entity_slideshow": "1.2",
        "drupal/migrate_plus": "3.0-beta1",
        "drupal/migrate_tools": "3.0-beta1",
        "drupal/url_embed": "1.0-alpha1",
        "drupal/views_bootstrap": "3.x-dev",
        "drupal/wxt_library": "1.x-dev",
        "drupal/wxt_bootstrap": "1.x-dev",
        "wet-boew/wet-boew": "4.0.23",
        "wet-boew/theme-wet-boew": "4.0.23",
        "wet-boew/theme-base": "4.0.23",
        "wet-boew/theme-gc-intranet": "4.0.23",
        "wet-boew/theme-gcwu-fegc": "4.0.23",
        "wet-boew/theme-gcweb": "4.0.23",
        "wet-boew/theme-ogpl": "4.0.23"
    }
}
